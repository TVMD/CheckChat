<resources>
    <declare-styleable name="StartView">
        <attr name="colorOutlineFrom" format="color" />
        <attr name="colorOutlineTo" format="color" />
        <attr name="colorFillFrom" format="color" />
        <attr name="colorFillTo" format="color" />
        <attr name="polygonVertices" format="integer" />
        <attr name="polygonRotation" format="integer" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FlexibleRatingBar">
        <attr name="space" format="dimension" />
        <attr name="numStars" format="integer" />
        <attr name="value" format="float" />
        <attr name="height_view" format="dimension" />
    </declare-styleable>

</resources>